{
  "name": "hackkings-app",
  "version": "1.0.0",
  "description": "Simple Node & React application for HackKings workshop.",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src/index.js\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist/index.js",
    "prestart": "npm run -s build",
    "test": "mocha --require babel-core/register 'test/**/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdm319/hackkings-app.git"
  },
  "keywords": [
    "node",
    "react",
    "nodejs",
    "reactjs",
    "hackkings",
    "simple",
    "tutorial"
  ],
  "author": "Chris Matthews <chris@matthews.digital>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdm319/hackkings-app/issues"
  },
  "homepage": "https://github.com/cdm319/hackkings-app#readme",
  "private": true,
  "babel": {
    "presets": ["es2015", "stage-0"]
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "nodemon": "^1.11.0"
  }
}
